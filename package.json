{
  "name": "query-data-to-chart-private",
  "version": "1.0.5",
  "description": "Draw charts from Kusto query data",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepublish": "npm run build && npm run test",
    "test": "npm run build && jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sigalvo/query-data-to-chart-private.git"
  },
  "keywords": [],
  "author": {
    "name": "Violet Voronetzky",
    "email": "sigalvo@microsoft.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sigalvo/query-data-to-chart-private/issues"
  },
  "homepage": "https://github.com/sigalvo/query-data-to-chart-private#readme",
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "typescript": "^3.7.2",
    "@types/jest": "^24.0.22",
    "jest": "^24.9.0",
    "@types/lodash": "^4.14.135",
    "lodash": "^4.17.15",
    "moment": "^2.24.0"
  },
  "jest": {
    "testMatch": [
      "**/dist/**/?(*.)(spec|test).js?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
